<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label "Song" %><br>
  <%= f.text_field :title %><br/>
  <%= f.label "Artist" %><br>
  <%= f.text_field :artist_name %><br/>
  <%= f.label "Genre" %><br>
  <%= f.collection_select :genre_id, @genres, :id, :name %></br>
  <input name="song[note_contents][]" id="song_notes_1">
  <input name="song[note_contents][]" id="song_notes_2">
  <input name="song[note_contents][]" id="song_notes_3">
  <input type="submit" value="Submit">
  <br />
  <hr/>
  <%= f.submit %>
<% end %>


